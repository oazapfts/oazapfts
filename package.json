{
  "name": "oazapfts",
  "version": "2.2.4",
  "description": "OpenApi TypeScript client generator",
  "main": "lib/index.js",
  "bin": {
    "oazapfts": "./lib/cli.js"
  },
  "scripts": {
    "test": "npm run generate-demo && jest",
    "build": "tsc",
    "generate-demo": "npm run prepare && ./lib/cli.js ./demo/petstore.json ./demo/api.ts",
    "prepare": "npm run build && chmod +x ./lib/cli.js"
  },
  "keywords": [
    "openapi",
    "typescript"
  ],
  "author": "Felix Gnass",
  "license": "MIT",
  "repository": "cellular/oazapfts",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.3",
    "form-data": "^3.0.0",
    "jest": "^25.1.0",
    "node-fetch": "^2.6.0",
    "openapi-types": "^1.3.5",
    "prettier": "^2.0.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^9.0.1",
    "lodash": "^4.17.15",
    "swagger2openapi": "^5.4.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "ts",
      "d.ts"
    ],
    "restoreMocks": true,
    "transform": {
      "\\.ts$": "ts-jest"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
