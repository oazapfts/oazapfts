{
  "name": "oazapfts",
  "version": "0.0.0-development",
  "description": "OpenApi TypeScript client generator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oazapfts/oazapfts",
    "directory": "packages/codegen"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "bin": {
    "oazapfts": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./generate": {
      "types": "./dist/generate.d.ts",
      "require": "./dist/generate.cjs",
      "default": "./dist/generate.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && vite build --mode esm && vite build --mode cjs && chmod +x dist/cli.js && chmod +x dist/cli.cjs",
    "prepack": "sed -i 's|dist/||g' package.json && mv dist/* ."
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.11.24",
    "@types/tapable": "^2.2.7"
  },
  "peerDependencies": {
    "@oazapfts/runtime": "*"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "swagger2openapi": "^7.0.8",
    "tapable": "^2.2.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "openapi",
    "typescript",
    "codegen",
    "oazapfts"
  ]
}
