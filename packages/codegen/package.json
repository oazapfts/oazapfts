{
  "name": "oazapfts",
  "version": "0.0.0-development",
  "description": "OpenApi TypeScript client generator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oazapfts/oazapfts",
    "directory": "packages/codegen"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "bin": {
    "oazapfts": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "require": "./dist/context.cjs",
      "default": "./dist/context.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "require": "./dist/plugin.cjs",
      "default": "./dist/plugin.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && vite build --mode esm && vite build --mode cjs && chmod +x dist/cli.js && chmod +x dist/cli.cjs"
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.23",
    "@types/minimist": "^1.2.5",
    "@types/tapable": "^2.3.0"
  },
  "peerDependencies": {
    "@oazapfts/runtime": "0.0.0-development || ^1.2.0"
  },
  "dependencies": {
    "@oazapfts/resolve": "0.0.0-development || ^1.0.0",
    "@apidevtools/swagger-parser": "^12.1.0",
    "lodash": "^4.17.23",
    "minimist": "^1.2.8",
    "tapable": "^2.3.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "openapi",
    "typescript",
    "codegen",
    "oazapfts"
  ]
}
